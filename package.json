{
  "name": "sign-bridge-api",
  "version": "1.0.0",
  "description": "Express Typescript with Sequelize",
  "main": "index.js",
  "scripts": {
    "db:migrate:create": "npx sequelize-cli migration:generate --name",
    "db:seed:create": "npx sequelize-cli seed:generate --name",
    "db:migrate:all": "npx sequelize-cli db:migrate",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:migrate:undo:one": "npx sequelize-cli db:migrate:undo --name",
    "db:seed:undo:one": "npx sequelize-cli db:seed:undo --seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jwt-simple": "^0.5.6",
    "moment": "^2.30.1",
    "mysql2": "^3.11.4",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.3",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
